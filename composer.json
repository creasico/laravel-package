{
    "name": "creasico/laravel-package",
    "description": "Laravel Package Template",
    "keywords": ["laravel", "package", "template"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Creasi.co",
            "email": "supports@creasi.co"
        }
    ],
    "support": {
        "source": "https://github.com/creasico/laravel-package"
    },
    "scripts": {
        "lint": "vendor/bin/phpcs --standard=PSR12 -n -p src",
        "test": "vendor/bin/phpunit --colors=always"
    },
    "autoload": {
        "psr-4": {
            "Creasi\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Creasi\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0|^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^7.5",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
