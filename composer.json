{
    "name": "creasi/laravel-package",
    "description": "Laravel Package Template",
    "keywords": ["laravel", "package", "template"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Creasi Developers",
            "email": "developers@creasi.co"
        }
    ],
    "support": {
        "source": "https://github.com/creasico/laravel-package"
    },
    "scripts": {
        "fix": [
            "pint --preset laravel"
        ],
        "test": [
            "vendor/bin/phpunit --colors=always --coverage-text"
        ]
    },
    "autoload": {
        "psr-4": {
            "Creasi\\Package\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Creasi\\Tests\\": "tests",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.1",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^8.5",
        "phpunit/phpunit": "^10.1"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Creasi\\Package\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
